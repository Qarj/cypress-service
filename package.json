{
    "name": "cypress-service",
    "version": "1.7.6",
    "description": "Post your application Cypress tests to a server you host and have it run them in parallel and store the results",
    "main": "index.js",
    "scripts": {
        "cypress:run": "cypress run --config-file cypress/cypress.json",
        "cypress:open": "cypress open --config-file cypress/cypress.json",
        "cypress:verify": "cypress verify",
        "cypress:version": "cypress version",
        "cypress:info": "cypress info",
        "cypress:cache:list": "cypress cache list",
        "list-videos": "ls -l cypress/videos",
        "check-videos": "ok-file 'cypress/videos/*.mp4'",
        "set-status": "set-status",
        "start": "node index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Qarj/cypress-service.git"
    },
    "keywords": [
        "cypress",
        "test",
        "server"
    ],
    "author": "Qarj",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Qarj/cypress-service/issues"
    },
    "homepage": "https://github.com/Qarj/cypress-service#readme",
    "dependencies": {
        "config": "^3.3.3",
        "connect-timeout": "^1.9.0",
        "cypress": "^8.5.0",
        "cypress-real-events": "^1.5.1",
        "cypress-service-client": "^1.2.5",
        "cypress-wait-until": "^1.7.1",
        "eslint-plugin-cypress": "^2.11.2",
        "express": "^4.17.1",
        "express-fileupload": "^1.2.1",
        "junit-report-merger": "^3.0.2",
        "mocha-junit-reporter": "^2.0.2",
        "mochawesome": "^7.0.1",
        "mochawesome-merge": "^4.2.1",
        "mochawesome-report-generator": "^6.0.1",
        "serve-index": "^1.9.1",
        "unzipper": "^0.10.11"
    },
    "devDependencies": {
        "eslint": "^7.24.0",
        "prettier": "2.2.1"
    }
}
